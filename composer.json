{
    "name": "arxy/graphql-codegen",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "webonyx/graphql-php": "dev-master",
        "nette/php-generator": "^4.0",
        "psr/log": "^1.0 | ^2.0 | ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "^4.23",
        "symfony/validator": "^5.4 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Arxy\\GraphQLCodegen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arxy\\GraphQLCodegen\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
